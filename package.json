{
  "name": "baywallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "cold:ios": "docker-compose up -d && yarn rn-setup && yarn bc:load-wallet && yarn bc:generate 1 && yarn ios",
    "cold:android": "docker-compose up -d && yarn rn-setup && yarn bc:load-wallet && yarn bc:generate 1 && yarn android",
    "start": "node rn-setup.js && react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rn-setup": "node rn-setup.js",
    "bc:create-wallet": "docker exec -it bitcoind bitcoin-cli --rpcwallet=baywallet --rpcport=18443 --rpcuser=baywallet --rpcpassword=baywallet createwallet baywallet",
    "bc:load-wallet": "docker exec -it bitcoind bitcoin-cli --rpcwallet=baywallet --rpcport=18443 --rpcuser=baywallet --rpcpassword=baywallet loadwallet baywallet",
    "bc:generate": "docker exec -it bitcoind bitcoin-cli --rpcwallet=baywallet --rpcport=18443 --rpcuser=baywallet --rpcpassword=baywallet -generate"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@eva-design/eva": "^2.1.1",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/bottom-tabs": "^6.5.3",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "@synonymdev/react-native-ldk": "0.0.79",
    "@ui-kitten/components": "^5.1.2",
    "@ui-kitten/eva-icons": "^5.1.2",
    "assert": "^1.5.0",
    "axios": "^1.2.2",
    "babel-preset-react-native": "^4.0.1",
    "bip32": "2.0.6",
    "bip39": "^3.0.4",
    "buffer": "^6.0.3",
    "crypto": "^1.0.1",
    "events": "^3.3.0",
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "mobx-react-lite": "^3.4.0",
    "process": "^0.11.10",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-crypto": "^2.2.0",
    "react-native-fs": "^2.20.0",
    "react-native-logs": "^5.0.1",
    "react-native-randombytes": "^3.6.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-svg": "^13.7.0",
    "react-native-tcp-socket": "^6.0.3",
    "readable-stream": "^1.0.33",
    "rn-electrum-client": "https://github.com/synonymdev/rn-electrum-client",
    "stream": "^0.0.2",
    "stream-browserify": "^1.0.0",
    "vm-browserify": "^0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "rn-nodeify": "^10.3.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  }
}
